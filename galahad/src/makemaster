                     ampl PRECIS=double PWD=$(PWD)/ampl \
            CUSTOM_AMPL_FLAGS="-DQP -DCQP -DQPA -DQPB \
                               -DQPC -DPRESOLVE -DFILTRANE -DLANCELOT" )

