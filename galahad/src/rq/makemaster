RTRSSS = $(OBJS)/trss.o -L$(OBJS) $(LIBS)
RTRSST = $(OBJS)/trst.o -L$(OBJS) $(LIBS)

RRQSDS = $(OBJD)/rqss.o -L$(OBJD) $(LIBS)
