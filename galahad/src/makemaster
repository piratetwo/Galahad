	( cd bsc ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) bsc \
                    PRECIS=double PWD=$(PWD)/bsc )

#  ---
#  bgo
#  ---

bgo_cutest: bgo_cutest_$(PRECIS)

bgo_cutest_single:
	( cd forthcoming/bgo ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
            bgo_cutest PRECIS=single PWD=$(PWD)/forthcoming/bgo )

bgo_cutest_double:
	( cd forthcoming/bgo ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
            bgo_cutest PRECIS=double PWD=$(PWD)/forthcoming/bgo )

bgo: bgo_$(PRECIS)

bgo_single:
	( cd forthcoming/bgo ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
            bgo PRECIS=single PWD=$(PWD)/forthcoming/bgo )

bgo_double:
	( cd forthcoming/bgo ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
            bgo PRECIS=double PWD=$(PWD)/forthcoming/bgo )

#  book keeping

clean:
	@printf ' %-9s\t\t' "Cleaning"
	$(RM) $(OBJ)/*
	@printf '[ OK ]\n'

cleanall:
	@printf ' %-14s\t\t' "Removing all $(PRECIS) precision object and module files"
	$(RM) $(OBJ)/* $(MOD)/*
	@printf '[ OK ]\n'

