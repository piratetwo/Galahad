	rm ../gmtr/run_gmtr

gmtrdt: $(GMTRDT)
	echo " Exhaustive test of subroutine interface to gmtr"
	cd ../gmtr ; $(FORTRAN) $(RUNFFLAGS) $(SPECIAL) -o run_gmtr $(RGMTRDT)
	- ../gmtr/run_gmtr >& ../gmtr/gmtrdt.output
	cat ../gmtr/gmtrdt.output
	rm ../gmtr/run_gmtr

