gltr_cutest: gltr_cutest_$(PRECIS)
	@printf ' %-21s\n' "GALAHAD: GLTR ($(PRECIS) $(SUCC_CUTEST)" 
gltr_cutest_single: $(SGLTR)
	$(RANLIB) $(LGS)
gltr_cutest_double: $(DGLTR)
	$(RANLIB) $(LGD)

