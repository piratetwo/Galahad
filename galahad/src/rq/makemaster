	@printf ' %-9s %-15s\t\t' "Compiling" "runrlls_sif" 
	$(SED) -f $(SEDS) ../rlls/runrlls_sif.f90 > $(OBJ)/runrlls_sif.f90
	cd $(OBJ); $(FORTRAN) -o runrlls_sif.o $(FFLAGSS) runrlls_sif.f90
	rm $(OBJ)/runrlls_sif.f90
	@printf '[ OK ]\n'

