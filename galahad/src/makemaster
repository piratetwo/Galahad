                     all PRECIS=single PWD=$(PWD)/matlab )
all_matlab_double:
	( cd matlab ; $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
                     all PRECIS=double PWD=$(PWD)/matlab )

