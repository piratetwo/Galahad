rqs_silent_double: $(RQSOBJECTSD)

rqs: rqs_$(PRECIS)
	@printf ' %-21s\n' "GALAHAD: RQS ($(PRECIS) $(SUCC)" 
rqs_single: $(RQSOBJECTSS)
	$(RANLIB) $(LGS)
rqs_double: $(RQSOBJECTSD)
	$(RANLIB) $(LGD)

rqssrunall: rqsss rqsst
rqsdrunall: rqsds rqsdt
rqsrunall: rqsdrunall rqssrunall

